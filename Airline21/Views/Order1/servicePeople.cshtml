@model List<Airline21.Models.service>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Service People</title>
    <!-- Add your CSS styles or include any necessary libraries here -->
</head>
<body>
    <h2>Service People</h2>
    <style>
        body {
            font-family: Arial, sans-serif;
        }

        h2 {
            color: #333;
        }

        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }

        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }

        th {
            background-color: #f2f2f2;
        }

        tr:hover {
            background-color: #f5f5f5;
        }
    </style>
    @if (Model != null && Model.Count > 0)
    {
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Extra Luggage</th>
                    <th>security Service</th>
                    <th>Total Service Cost</th>              
                </tr>
            </thead>
            <tbody>
                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.Id</td>
                        <td>@user.Name</td>
                        <td>@user.extraluggage</td>
                        <td>@user.securityService</td>
                        <td>@user.TotalServicePeople</td>
                    </tr>
           
                }

            </tbody>
        </table>
    }
    else
    {
        <p>No service people data found in the session.</p>
    }

    <!-- Add your additional HTML content or JavaScript scripts here -->

</body>
</html>